{
  "containerDefinitions": [
    {
      "name": "concon-container",
      "image": "${AWS_ACCOUNT_ID}.dkr.ecr.ap-northeast-2.amazonaws.com/concon:b6499f5c20a857c934ecfda4bf45c6da614892e0",
      "cpu": 2048,
      "memory": 5120,
      "memoryReservation": 4096,
      "portMappings": [
        {
          "name": "concon-container-8080-tcp",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "${SPRING_DATASOURCE_URL}"
        },
        {
          "name": "SPRING_DATASOURCE_USERNAME",
          "value": "${SPRING_DATASOURCE_USERNAME}"
        },
        {
          "name": "SPRING_DATASOURCE_PASSWORD",
          "value": "${SPRING_DATASOURCE_PASSWORD}"
        },
        {
          "name": "SPRING_DATA_REDIS_HOST",
          "value": "${SPRING_DATA_REDIS_HOST}"
        },
        {
          "name": "SPRING_DATA_REDIS_PORT",
          "value": "${SPRING_DATA_REDIS_PORT}"
        },
        {
          "name": "SPRING_JWT_SECRET",
          "value": "${SPRING_JWT_SECRET}"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/concon-task2",
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "max-buffer-size": "25m",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "family": "concon-task2",
  "taskRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/${IAM_ROLE_NAME}",
  "executionRoleArn": "arn:aws:iam::${AWS_ACCOUNT_ID}:role/${IAM_ROLE_NAME}",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "2048",
  "memory": "6144",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}